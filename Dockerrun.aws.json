{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "tarun-first-api",
      "image": "tarunbot/tarun-first-api",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environments": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://db:5432/customer"
        }
      ]
    }
  ]
}